<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="457.0" prefWidth="575.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MiniprojectController">
   <children>
      <Button fx:id="login" layoutX="265.0" layoutY="338.0" mnemonicParsing="false" onAction="#handleButtonClick" prefHeight="25.0" prefWidth="58.0" text="Sign Up">
         <font>
            <Font name="Microsoft Tai Le" size="9.0" />
         </font>
      </Button>
      <TextField fx:id="username" alignment="CENTER" layoutX="211.0" layoutY="172.0" onAction="#usernameSet" prefHeight="27.0" prefWidth="167.0" promptText="Username">
         <font>
            <Font name="Eras Medium ITC" size="12.0" />
         </font>
      </TextField>
      <TextField fx:id="password" alignment="CENTER" layoutX="211.0" layoutY="218.0" onAction="#passwordSet" promptText="Password" />
      <Text layoutX="175.0" layoutY="103.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome.">
         <font>
            <Font name="Eras Bold ITC" size="51.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
