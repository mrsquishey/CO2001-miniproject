<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="532.0" prefWidth="575.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MiniprojectController">
   <children>
      <Button fx:id="signup" alignment="CENTER" contentDisplay="CENTER" layoutX="266.0" layoutY="464.0" mnemonicParsing="false" onAction="#handleButtonClick" prefHeight="25.0" prefWidth="58.0" text="Sign Up" textFill="#8d8d8d">
         <font>
            <Font name="Microsoft Tai Le" size="9.0" />
         </font>
      </Button>
      <TextField fx:id="username" alignment="CENTER" layoutX="211.0" layoutY="172.0" onAction="#usernameSet" prefHeight="27.0" prefWidth="167.0" promptText="Username">
         <font>
            <Font name="Eras Medium ITC" size="12.0" />
         </font>
      </TextField>
      <TextField fx:id="password" alignment="CENTER" layoutX="211.0" layoutY="218.0" onAction="#passwordSet" prefHeight="27.0" prefWidth="167.0" promptText="Password" />
      <Text layoutX="175.0" layoutY="103.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome.">
         <font>
            <Font name="Eras Bold ITC" size="51.0" />
         </font>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </Text>
      <Button fx:id="login" onAction="#clickLogin" layoutX="269.0" layoutY="266.0" mnemonicParsing="false" text="Log in" />
   </children>
</AnchorPane>
