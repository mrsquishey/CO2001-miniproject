<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="532.0" prefWidth="575.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SignUp">
   <children>
      <Text layoutX="201.0" layoutY="88.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Sign Up">
         <font>
            <Font name="Helvetica Neue" size="59.0" />
         </font>
      </Text>
      <TextField fx:id="username" alignment="CENTER" layoutX="220.0" layoutY="189.0" promptText="Username" />
      <TextField fx:id="password" alignment="CENTER" layoutX="220.0" layoutY="252.0" promptText="Password" />
      <RadioButton fx:id="male" layoutX="173.0" layoutY="356.0" mnemonicParsing="false" selected="true" text="Male" AnchorPane.bottomAnchor="159.0">
         <toggleGroup>
            <ToggleGroup fx:id="group" />
         </toggleGroup></RadioButton>
      <RadioButton fx:id="female" layoutX="272.0" layoutY="355.0" mnemonicParsing="false" text="Female" toggleGroup="$group" />
      <RadioButton fx:id="other" layoutX="383.0" layoutY="355.0" mnemonicParsing="false" text="Other" toggleGroup="$group" />
      <Button fx:id="signup" layoutX="273.0" layoutY="432.0" mnemonicParsing="false" onAction="#handleButtonClick" text="Sign Up" />
   </children>
</AnchorPane>
